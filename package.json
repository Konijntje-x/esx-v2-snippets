{
    "name": "esx-v2-snippets",
    "displayName": "ESX-V2-snippets",
    "description": "Snippets for ESX2, made with a partnership with ESX-framework. This extension has ESX V2 snippets and all the GTA vehicles spawnnames and GTA weapon 'spawn' names. They are all separate available.",
    "version": "1.1.0",
    "publisher": "Konijntje-x",
    "icon": "esx.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Konijntje-x/esx-v2-snippets.git"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "lua",
                "path": "./snippets/weapons.json"
            },
            {
                "language": "lua",
                "path": "./snippets/vehicles.json"
            },
            {
                "language": "c#",
                "path": "./snippets/weapons.json"
            },
            {
                "language": "c#",
                "path": "./snippets/vehicles.json"
            },
            {
                "language": "js",
                "path": "./snippets/vehicles.json"
            },
            {
                "language": "js",
                "path": "./snippets/weapons.json"
            },
            {
                "language": "c#",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "js",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
